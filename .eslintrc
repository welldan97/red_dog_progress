{
  "extends": "airbnb",
  "plugins": [
    "jest"
  ],
  "parser": "babel-eslint",
  "rules": {
    "max-len": ["error", 80],
    "no-mixed-operators": 0,
    "arrow-parens": ["error", "as-needed"],
    "react/jsx-filename-extension": 0,
    "react/react-in-jsx-scope": 0
  },
  "env": {
    "jest": true
  }
}
